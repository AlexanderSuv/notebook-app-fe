{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": [
      false,
      {
        "react": "React",
        "react-dom": "ReactDOM"
      }
    ],
    "ter-indent": false,
    "trailing-comma": false,
    "ter-arrow-parens": false,
    "function-name": false,
    "variable-name": false,
    "max-line-length": [
      true,
      160
    ],
    "align": false
  }
}
